target C {
    platform: "RP2040",
    threading: false
}

import Led from "./lib/Led.lf";

preamble {=
    #include <pico/stdlib.h>
    #include <hardware/gpio.h>
=}

main reactor {
    timer t1(0, 100 msec);
    led = new Led();
    reaction(t1) -> led.tog {=
        lf_set(led.tog, true);
    =}
}
